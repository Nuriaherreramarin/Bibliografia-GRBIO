   note = {SGR2022: YES ; PID: NO ; HQ: 2 ; IF: 3222 ; Firstauthor: NO ; Lastauthor: NO ; Bioinf: 0 ; Stat_Prob: 0 ;  ;},
   pages = {385-394},
   title = {Leveraging Data Science for a Personalized Haemodialysis},
   volume = {6},
   url = {https://www.karger.com/Article/FullText/507291},
   year = {2020},
}
