@article{Ferreira2018,
   abstract = {<p>Post-mortem tissues samples are a key resource for investigating patterns of gene expression. However, the processes triggered by death and the post-mortem interval (PMI) can significantly alter physiologically normal RNA levels. We investigate the impact of PMI on gene expression using data from multiple tissues of post-mortem donors obtained from the GTEx project. We find that many genes change expression over relatively short PMIs in a tissue-specific manner, but this potentially confounding effect in a biological analysis can be minimized by taking into account appropriate covariates. By comparing ante- and post-mortem blood samples, we identify the cascade of transcriptional events triggered by death of the organism. These events do not appear to simply reflect stochastic variation resulting from mRNA degradation, but active and ongoing regulation of transcription. Finally, we develop a model to predict the time since death from the analysis of the transcriptome of a few readily accessible tissues.</p>},
   author = {Pedro G. Ferreira and Manuel Muñoz-Aguirre and Ferran Reverter and Caio P. Sá Godinho and Abel Sousa and Alicia Amadoz and Reza Sodaei and Marta R. Hidalgo and Dmitri Pervouchine and Jose Carbonell-Caballero and Ramil Nurtdinov and Alessandra Breschi and Raziel Amador and Patrícia Oliveira and Cankut Çubuk and João Curado and François Aguet and Carla Oliveira and Joaquin Dopazo and Michael Sammeth and Kristin G. Ardlie and Roderic Guigó},
   doi = {10.1038/s41467-017-02772-x},
   issn = {2041-1723},
   issue = {1},
   journal = {Nature Communications},
   month = {2},
   note = {SGR2022: YES ; PID: NO ; HQ: 1 ; IF: 14,919 ; Firstauthor: NO ; Lastauthor: NO ; Bioinf: 0 ; Stat_Prob: 0 ;  ;},
   pages = {490},
   title = {The effects of death and post-mortem cold ischemia on human tissue transcriptomes},
   volume = {9},
   url = {https://www.nature.com/articles/s41467-017-02772-x},
   year = {2018},
}
