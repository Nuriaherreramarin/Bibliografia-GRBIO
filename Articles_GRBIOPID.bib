   month = {9},
   note = {SGR2022: YES ; PID: NO ; HQ: 1 ; IF: 4,011 ; Firstauthor: NO ; Lastauthor: NO ; Bioinf: 0 ; Stat_Prob: 0 ;  ;},
   pages = {13193},
   title = {Normal limits of home measured spatial gait parameters of the elderly population and their association with health variables},
   volume = {8},
