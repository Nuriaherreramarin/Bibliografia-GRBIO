@article{,
   abstract = {<p> <bold>Background</bold> : Precision medicine is the Holy Grail of interventions that are tailored to a patient’s individual characteristics. However, conventional clinical trials are designed to find differences in averages, and interpreting these differences depends on untestable assumptions. Although only an ideal, a constant effect of treatment would facilitate individual management. A direct consequence of a constant effect is that the variance of the outcome measure would be the same in the treated and control arms. We reviewed the literature to explore the similarity of these variances as a foundation for examining whether and how often precision medicine is definitively required. </p>},
   author = {Jordi Cortés and José Antonio González and María Nuncia Medina and Markus Vogler and Marta Vilaró and Matt Elmore and Stephen John Senn and Michael Campbell and Erik Cobo},
