   issue = {1},
   journal = {Scientific Reports},
   month = {5},
   note = {SGR2022: YES ; PID: YES ; HQ: 1 ; IF: 4,379 ; Firstauthor: NO ; Lastauthor: NO ; Bioinf: 0 ; Stat_Prob: 0 ;  ;},
   pages = {11289},
