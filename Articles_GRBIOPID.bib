   month = {7},
   note = {SGR2022: YES ; PID: YES ; HQ: 1 ; IF: 9,043 ; Firstauthor: NO ; Lastauthor: NO ; Bioinf: 0 ; Stat_Prob: 0 ;  ;},
   pages = {1047-1059},
   title = {A limited set of transcriptional programs define major cell types},
