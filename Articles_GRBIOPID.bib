   issn = {0028-3835},
   issue = {4},
   journal = {Neuroendocrinology},
   note = {SGR2022: YES ; PID: NO ; HQ: 1 ; IF: 6,804 ; Firstauthor: NO ; Lastauthor: NO ; Bioinf: 0 ; Stat_Prob: 0 ;  ;},
   pages = {315-323},
   title = {Identifying Prognostic Factors for Well-Differentiated Metastatic Pancreatic Neuroendocrine Tumours: A Retrospective International Multicentre Cohort Study},
   volume = {107},
   url = {https://www.karger.com/Article/FullText/492223},
   year = {2018},
}
